<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/privacyPolicyActivityId"
    android:overScrollMode="always"
    android:fillViewport="true"
    tools:context="PrivacyPolicyActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:orientation="vertical">

        <TextView
            android:id="@+id/privacyPolicyDescription"
            android:text="@string/privacyPolicyDescriptionString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_body_1_material"
            android:layout_marginTop="18dp" />

        <TextView
            android:id="@+id/applicationInformationHeader"
            android:text="@string/applicationInformationHeaderString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_medium_material"
            android:layout_below="@+id/privacyPolicyDescription"
            android:layout_alignLeft="@+id/privacyPolicyDescription"
            android:layout_alignStart="@+id/privacyPolicyDescription"
            android:layout_marginTop="18dp" />

        <TextView
            android:id="@+id/userProvidedInformationHeader"
            android:text="@string/userProvidedInformationHeaderString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize = "@dimen/abc_text_size_subhead_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/applicationInformationHeader"
            android:layout_alignLeft="@+id/applicationInformationHeader"
            android:layout_alignStart="@+id/applicationInformationHeader" />

        <TextView
            android:id="@+id/userProvidedInformationDescription"
            android:text="@string/userProvidedInformationDescriptionString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_body_1_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/userProvidedInformationHeader"
            android:layout_alignLeft="@+id/userProvidedInformationHeader"
            android:layout_alignStart="@+id/userProvidedInformationHeader" />

        <TextView
            android:id="@+id/automaticallyCollectedInformationHeader"
            android:text="@string/automaticallyCollectedInformationHeaderString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_subhead_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/userProvidedInformationDescription"
            android:layout_alignLeft="@+id/userProvidedInformationDescription"
            android:layout_alignStart="@+id/userProvidedInformationDescription" />

        <TextView
            android:id="@+id/automaticallyCollectedInformationDescription"
            android:text="@string/automaticallyCollectedInformationDescriptionHeader"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_body_1_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/automaticallyCollectedInformationHeader"
            android:layout_alignLeft="@+id/automaticallyCollectedInformationHeader"
            android:layout_alignStart="@+id/automaticallyCollectedInformationHeader" />

        <TextView
            android:id="@+id/locationInformationHeader"
            android:text="@string/locationInformationHeaderString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_medium_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/automaticallyCollectedInformationDescription"
            android:layout_alignLeft="@+id/automaticallyCollectedInformationDescription"
            android:layout_alignStart="@+id/automaticallyCollectedInformationDescription" />

        <TextView
            android:id="@+id/locationInformationDescription"
            android:text="@string/locationInformationDescriptionString"
