            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_medium_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/automaticAdvertisingDescription"
            android:layout_alignLeft="@+id/automaticAdvertisingDescription"
            android:layout_alignStart="@+id/automaticAdvertisingDescription" />

        <TextView
            android:id="@+id/dataRententionDescription"
            android:text="@string/dataRententionDescriptionString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_body_1_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/dataRententionHeader"
            android:layout_alignLeft="@+id/dataRententionHeader"
            android:layout_alignStart="@+id/dataRententionHeader" />

        <TextView
            android:id="@+id/childrenHeader"
            android:text="@string/childrenHeaderString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_medium_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/dataRententionDescription"
            android:layout_alignLeft="@+id/dataRententionDescription"
            android:layout_alignStart="@+id/dataRententionDescription" />

        <TextView
