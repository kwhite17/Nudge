            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_medium_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/changesDescription"
            android:layout_alignLeft="@+id/changesDescription"
            android:layout_alignStart="@+id/changesDescription"/>

        <TextView
            android:id="@+id/consentDescription"
            android:text="@string/consentDescriptionString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/abc_text_size_body_1_material"
            android:layout_marginTop="18dp"
            android:layout_below="@+id/consentHeader"
            android:layout_alignLeft="@+id/consentHeader"
            android:layout_alignStart="@+id/consentHeader"/>

        <TextView
